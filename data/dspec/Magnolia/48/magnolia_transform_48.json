            "range": [48, 1],
            "outKey": "PL_WNDIR_F48",
            "dataIntegrityCall": {
                "call": "AngleInterpolation",
                "args": {
                    "limit":"10800",
                    "method":"linear",
                    "limit_area":"inside"    
                }
            }
        },
        { 
