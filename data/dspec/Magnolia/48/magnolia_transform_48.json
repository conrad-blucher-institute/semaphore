                "call": "PandasInterpolation",
                "args": {
                    "limit":"10800",
                    "method":"linear",
                    "limit_area":"inside"    
                }
            }
        },
        { 
            "_name": "Harmonic Predictions",
            "location": "PortLavaca",
            "source": "LIGHTHOUSE",
            "series": "pHarm",
            "unit": "meter",
            "datum": "MLLW",
            "interval": 3600,
            "range":  [43, 31],
            "outKey": "PL_pHarm_48"
        },
        { 
            "_name": "Harmonic Predictions POC",
            "location": "PortOConnor",
            "source": "LIGHTHOUSE",
            "series": "pHarm",
            "unit": "meter",
            "datum": "MLLW",
            "interval": 3600,
            "range":  [43, 31],
            "outKey": "POC_pHarm_48"
        }

    ],
    "postProcessCall": [
